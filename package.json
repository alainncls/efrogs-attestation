{
  "name": "efrogs-attestation",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm -r run dev",
    "format": "prettier --check '**/*.sol' '**/*.ts' '**/*.tsx' '**/*.json' '**/*.md' --ignore-path .prettierignore",
    "format:fix": "prettier --write '**/*.sol' '**/*.ts' '**/*.tsx' '**/*.json' '**/*.md' --ignore-path .prettierignore",
    "preinstall": "npx only-allow@1.2.2 pnpm",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lint:fix",
    "test": "pnpm -r run test",
    "typecheck": "pnpm -r run typecheck"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "prettier-plugin-solidity": "^2.2.1"
  },
  "packageManager": "pnpm@10.26.2",
  "engines": {
    "node": ">=22.21.1",
    "pnpm": ">=10"
  }
}
